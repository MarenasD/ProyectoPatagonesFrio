@model proyecto8.Models.proveedor

@{
    ViewBag.Title = "Details";
}

<h2>Detalle del Proveedor</h2>

<div>
    <h4></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Nombre: @*@Html.DisplayNameFor(model => model.nombre_proveedor)*@
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre_proveedor)
        </dd>

        <dt>
           Direccion: @*@Html.DisplayNameFor(model => model.direccion_proveedor)*@
        </dt>

        <dd>
            @Html.DisplayFor(model => model.direccion_proveedor)
        </dd>

        <dt>
           Telefono: @*@Html.DisplayNameFor(model => model.telefono_proveedor)*@
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefono_proveedor)
        </dd>

        <dt>
          Email:  @*@Html.DisplayNameFor(model => model.email_proveedor)*@
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email_proveedor)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.rut_ptoveedor }) |
